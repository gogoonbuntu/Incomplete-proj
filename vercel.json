{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "silent": true
  },
  "crons": [
    {
      "path": "/api/cron/summary-updater",
      "schedule": "* * * * *"
    }
  ]
}
